{
  "name": "Social Time Tracker",
  "manifest_version": 2,
  "version": "0.1",

  "description": "Shows you how many times you go to social media sites",

  "permissions": [
    "tabs",
    "<all_urls>"
  ],

  "browser_action": {
    "default_title": "History Watcher"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
